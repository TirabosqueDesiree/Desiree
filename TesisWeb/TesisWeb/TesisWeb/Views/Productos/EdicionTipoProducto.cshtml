
@{
    ViewBag.Title = "EdicionTipoProducto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model TesisWeb.Models.clasesProducto.TipoProducto
<h2>Editar Tipo Producto</h2>

<div class="row">
    <div class="col-md-2">
    </div>
    @using (Html.BeginForm())
    {
        @Html.HiddenFor(x => x.IdTipoProducto)

        <div class="col-md-8">

            <div class="form-group">
                <label>Descripción</label>
                @Html.TextBoxFor(x => x.Descripcion, htmlAttributes: new { @class = "form-control" })

            </div>
            <input type="submit" class="btn btn-primary" value="Editar Tipo Producto" />
        </div>
    }
    <div class="col-md-2">
    </div>
</div>



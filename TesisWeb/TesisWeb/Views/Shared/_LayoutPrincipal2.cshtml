    <title>J&A Carnes SA</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
    @Styles.Render("~/assets /img/favicon.png")
    @Styles.Render("~/assets/img/apple-touch-icon.png")
    <!--ver-->
    @Styles.Render("~/Content/templatePrinc/assets/vendor/bootstrap/css/bootstrap.min.css")
    @Styles.Render("~/Content/templatePrinc/assets/vendor/icofont/icofont.min.css")
    @Styles.Render("~/Content/templatePrinc/assets/vendor/boxicons/css/boxicons.min.css")
    @Styles.Render("~/Content/templatePrinc/assets/vendor/owl.carousel/assets/owl.carousel.min.css")
    @Styles.Render("~/Content/templatePrinc/assets/vendor/remixicon/remixicon.css")
    @Styles.Render("~/Content/templatePrinc/assets/vendor/venobox/venobox.css")
    @Styles.Render("~/Content/templatePrinc/assets/vendor/aos/aos.css")
    @Styles.Render("~/Content/templatePrinc/assets/css/style.css")

    @Styles.Render("~/Content/fontawesome-free-5.15.3-web/css/fontawesome.css")

</head>

<body>
    



    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top">

        <div class="container-fluid">
            <div class="row justify-content-center">
                <div class="col-xl-10 d-flex align-items-center">
                    <h1 class="logo mr-auto"><a href="index.html">J&A Carnes SA<span>.</span></a></h1>
                    <!-- Uncomment below if you prefer to use an image logo -->
                    <!-- <a href="index.html" class="logo mr-auto"><img src="assets/img/logo.png" alt=""></a>-->

                    <nav class="nav-menu d-none d-lg-block">
                        <ul>
                            <li class="active"><a href="#header">Home</a></li>
                            <li><a href="#about">Conocenos</a></li>
                            <li><a href="#services">Repartos</a></li>

                            <!--<li><a href="#team">Team</a></li>-->

                            <li class="drop-down">
                                <a href="">Drop Down</a>
                                <ul>
                                    <li><a href="#">Drop Down 1</a></li>
                                    <li class="drop-down">
                                        <a href="#">Productos</a>
                                        <ul>
                                            <li><a href="#">Deep Drop Down 1</a></li>
                                            <li><a href="#">Deep Drop Down 2</a></li>
                                            <li><a href="#">Deep Drop Down 3</a></li>
                                            <li><a href="#">Deep Drop Down 4</a></li>
                                            <li><a href="#">Deep Drop Down 5</a></li>


                                        </ul>
                                    </li>
                                    <li><a href="#">Drop Down 2</a></li>
                                    <li><a href="#">Drop Down 3</a></li>
                                    <li><a href="#">Drop Down 4</a></li>
                                </ul>
                            </li>
                            <li><a href="#contact">Contact</a></li>

                            <li>
                                @Html.ActionLink("Empresa", "LoginAdm", "Acceso")
                            </li>

                            <li>@Html.ActionLink("Crear Cuenta", "CrearUserCliente", "ClienteUsuario")</li>
                            <li>@Html.ActionLink("Pedidos", "Index", "MaestroDetalle")</li>

                            @if (Session["User"] == null)
                            {
                                <li>@Html.ActionLink("Iniciar Sesión", "Login", "Acceso")</li>
                            }
                            else
                            {

                                <li>
                                    <a href="@Url.Action("Logout", "Acceso" )"><i class="fas fa-user-times"></i></a>


                                </li>


                            }

                        </ul>
                    </nav><!-- .nav-menu -->

                    <a href="#about" class="get-started-btn scrollto">Get Started</a>
                </div>
            </div>

        </div>

    </header>
    @RenderBody()

    @Scripts.Render("~/Content/fontawesome-free-5.15.3-web/js/all.min.js")
    @Scripts.Render("~/Content/fontawesome-free-5.15.3-web/js/fontawesome.min.js")

    @Scripts.Render("~/bundles/modernizr")
